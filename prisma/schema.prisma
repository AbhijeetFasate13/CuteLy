generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Url {
  slug           String    @unique
  originalUrl    String
  createdAt      DateTime  @default(now())
  hitCount       Int       @default(0)
  lastAccessedAt DateTime?
  id             Int       @id @default(autoincrement())
}
